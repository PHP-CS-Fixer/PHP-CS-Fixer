--TEST--
Integration of fixers: function_declaration,method_argument_space.
--RULESET--
{"function_declaration": true, "method_argument_space": true}
--REQUIREMENTS--
{"php": 80000}
--EXPECT--
<?php
function foo($bar, ) { echo $bar.$baz; };
$a = function () use ($b, $c, ): void { echo $b.$c; };

--INPUT--
<?php
function foo($bar,) { echo $bar.$baz; };
$a = function () use ($b, $c,): void { echo $b.$c; };
