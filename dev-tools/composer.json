{
    "type": "project",
    "require": {
        "php": "^8.4",
        "ergebnis/composer-normalize": "^2.45.0",
        "icanhazstring/composer-unused": "^0.8.11",
        "kubawerlos/composer-smaller-lock": "^1.1.0",
        "maglnet/composer-require-checker": "^4.15.0",
        "mi-schi/phpmd-extension": "^4.3.0",
        "phpcompatibility/php-compatibility": "10.x-dev as 9.99.0",
        "phpcompatibility/phpcompatibility-symfony": "^1.2.2",
        "phpmd/phpmd": "^2.15.0",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "^2.1.2",
        "phpstan/phpstan-phpunit": "^2.0.4",
        "phpstan/phpstan-strict-rules": "^2.0.3",
        "phpstan/phpstan-symfony": "^2.0.2"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": false,
            "kubawerlos/composer-smaller-lock": true,
            "phpstan/extension-installer": true
        },
        "optimize-autoloader": true,
        "platform": {
            "php": "8.4"
        },
        "prefer-stable": true,
        "sort-packages": true
    }
}
