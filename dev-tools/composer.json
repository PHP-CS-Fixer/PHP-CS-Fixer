{
    "type": "project",
    "require": {
        "php": "^8.5",
        "ergebnis/composer-normalize": "^2.50.0",
        "kubawerlos/composer-smaller-lock": "^1.1.0",
        "mi-schi/phpmd-extension": "^4.3.0",
        "phpcompatibility/php-compatibility": "^10-alpha2",
        "phpcompatibility/phpcompatibility-symfony": "^2-alpha1",
        "phpmd/phpmd": "dev-master as 2.16.0",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "^2.1.38",
        "phpstan/phpstan-phpunit": "^2.0.12",
        "phpstan/phpstan-strict-rules": "^2.0.8",
        "phpstan/phpstan-symfony": "^2.0.12",
        "shipmonk/composer-dependency-analyser": "^1.8.4",
        "shipmonk/dead-code-detector": "^0.14.1",
        "shipmonk/phpstan-baseline-per-identifier": "^2.3.0"
    },
    "minimum-stability": "alpha",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": false,
            "kubawerlos/composer-smaller-lock": true,
            "phpstan/extension-installer": true
        },
        "optimize-autoloader": true,
        "platform": {
            "php": "8.5"
        },
        "prefer-stable": true,
        "sort-packages": true
    }
}
