{
    "type": "project",
    "require": {
        "php": "^8.4",
        "ergebnis/composer-normalize": "^2.48.2",
        "kubawerlos/composer-smaller-lock": "^1.1.0",
        "mi-schi/phpmd-extension": "^4.3.0",
        "phpcompatibility/php-compatibility": "10.x-dev as 9.99.0",
        "phpcompatibility/phpcompatibility-symfony": "^1.2.2",
        "phpmd/phpmd": "dev-master as 2.16.0",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "^2.1.31",
        "phpstan/phpstan-phpunit": "^2.0.7",
        "phpstan/phpstan-strict-rules": "^2.0.7",
        "phpstan/phpstan-symfony": "^2.0.8",
        "shipmonk/composer-dependency-analyser": "^1.8.3",
        "shipmonk/dead-code-detector": "^0.13.4",
        "shipmonk/phpstan-baseline-per-identifier": "^2.2.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": false,
            "kubawerlos/composer-smaller-lock": true,
            "phpstan/extension-installer": true
        },
        "optimize-autoloader": true,
        "platform": {
            "php": "8.4"
        },
        "prefer-stable": true,
        "sort-packages": true
    }
}
